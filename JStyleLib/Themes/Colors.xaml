<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BaseButtonColor">#D58585</Color>
    <Color x:Key="BaseSystemColor">#DA434E</Color>
    <SolidColorBrush x:Key="BaseButtonBrush" Color="{StaticResource BaseButtonColor}" options:Freeze="true"/>
    <SolidColorBrush x:Key="BaseSystemBrush" Color="{StaticResource BaseSystemColor}" options:Freeze="true"/>
    <SolidColorBrush x:Key="BaseBorderBrush" Color="#FFD0A1A5" options:Freeze="true"/>
    <SolidColorBrush x:Key="TransparentBrush" Color="#00000000" options:Freeze="true"/>
    <SolidColorBrush x:Key="WaterMarkBrush" Color="#AAFFFFFF" options:Freeze="true"/>

    <SolidColorBrush x:Key="SysTabBackGroundBrush" Color="#DA434E" Opacity="0" options:Freeze="true"/>
    <SolidColorBrush x:Key="SysTabBackGroundMarkBrush" Color="#A0FFFFFF" options:Freeze="true"/>
</ResourceDictionary>